{
  "name": "datstat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:optimize": "psql $DATABASE_URL < prisma/migrations/add_performance_indexes.sql",
    "monitor:start": "tsx src/lib/performance/startup.ts",
    "monitor:health": "curl http://localhost:3000/api/monitoring/health",
    "monitor:metrics": "curl http://localhost:3000/api/monitoring/metrics",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.config.ts --reporter=verbose src/__tests__/unit",
    "test:integration": "vitest run --config vitest.config.ts --reporter=verbose src/__tests__/integration",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@as-integrations/next": "^3.2.0",
    "@prisma/client": "^6.1.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "graphql": "^16.10.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "happy-dom": "^18.0.1",
    "jest-mock-extended": "^4.0.0",
    "msw": "^2.10.4",
    "playwright": "^1.54.1",
    "prisma": "^6.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^2.0.2"
  }
}
